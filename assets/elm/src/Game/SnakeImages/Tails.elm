module Game.SnakeImages.Tails exposing (..)

import Svg.Styled as Svg exposing (..)
import Svg.Styled.Attributes as Attrs exposing (..)


getSnakeTail : String -> List (Attribute msg) -> List (Svg msg) -> Svg msg
getSnakeTail tailType =
    case tailType of
        "block-bum" ->
            blockBum

        "curled" ->
            curled

        "fat-rattle" ->
            fatRattle

        "freckled" ->
            freckled

        "pixel" ->
            pixel

        "regular" ->
            regular

        "round-bum" ->
            roundBum

        "skinny" ->
            skinny

        "small-rattle" ->
            smallRattle

        _ ->
            regular


blockBum : List (Attribute msg) -> List (Svg msg) -> Svg msg
blockBum attrs children =
    g (attrs ++ [ viewBox "0 0 1 1", transform "scale(1.4)" ])
        (children
            ++ [ Svg.path
                    [ d "M0 0h72v72H0z" ]
                    []
               ]
        )


